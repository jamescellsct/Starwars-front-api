{
  "name": "swapiweb",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "scripts": {
    "start": "npm-run-all -p watch:back start:front",
    "watch:back": "nodemon --exec \"npm run start:back\" api --ignore api/fakeData/db.json",
    "prestart:back": "npx babel-node ./api/generateFakeData.js --presets @babel/preset-env",
    "start:back": "npx babel-node ./api/index.js --presets @babel/preset-env",
    "start:front": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "reactstrap": "^7.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "route-auth": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "chance": "^1.0.18",
    "express": "^4.16.4",
    "json-schema-faker": "^0.5.0-rc16",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1"
  }
}
